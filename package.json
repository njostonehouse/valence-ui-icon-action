{
  "name": "vui-image-action",
  "version": "1.0.0",
  "description": "Polymer web component for applying image action styles",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-image-action.git"
  },
  "scripts": {
    "prebuild": "cpy image-action.html ./bower_components/temp",
    "build": "vulcanize ./bower_components/temp/image-action.html > ./test/image-action.test.html",
    "postbuild": "rimraf ./bower_components/temp",
    "clean": "rimraf ./test/image-action.test.html",
    "demo": "polyserve",
    "postinstall": "bower install",
    "snapshot": "phantomjs ./screenshots/build.js",
    "test": "polylint"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui",
    "icon",
    "action"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "cpy-cli": "^1.0.0",
    "csslint": "^0.10.0",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.4",
    "polylint": "^2.10.1",
    "polyserve": "^0.7.1",
    "rimraf": "^2.4.4",
    "vulcanize": "^1.14.8",
    "web-component-tester": "^4.2.2"
  },
  "dependencies": {
    "bower": "^1.7.7"
  }
}
